{
  "name": "fhir-questionnaire-converter",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/fhir-questionnaire-converter/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/fhir-questionnaire-converter",
        "main": "libs/fhir-questionnaire-converter/src/index.ts",
        "tsConfig": "libs/fhir-questionnaire-converter/tsconfig.lib.json",
        "assets": ["libs/fhir-questionnaire-converter/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {}
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/fhir-questionnaire-converter/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --write \"libs/fhir-questionnaire-converter/**/*.{js,ts,json,md}\""
      }
    },
    "format:check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --check \"libs/fhir-questionnaire-converter/**/*.{js,ts,json,md}\""
      }
    }
  }
}
