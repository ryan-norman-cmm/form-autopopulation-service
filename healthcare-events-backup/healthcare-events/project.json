{
  "name": "healthcare-events",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/healthcare-events/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": ["^build"],
      "options": {
        "outputPath": "dist/apps/healthcare-events",
        "main": "apps/healthcare-events/src/main.ts",
        "tsConfig": "apps/healthcare-events/tsconfig.app.json",
        "assets": []
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "healthcare-events:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "healthcare-events:build:development"
        },
        "production": {
          "buildTarget": "healthcare-events:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "config": "apps/healthcare-events/vitest.config.ts"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "apps/healthcare-events"
      }
    },
    "docker:build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker build -f apps/healthcare-events/Dockerfile --target development -t healthcare-events:dev .",
        "cwd": "{workspaceRoot}"
      }
    },
    "docker:build:prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker build -f apps/healthcare-events/Dockerfile --target production -t healthcare-events:prod .",
        "cwd": "{workspaceRoot}"
      },
      "dependsOn": ["build"]
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --write \"apps/healthcare-events/**/*.{js,ts,json,md}\"",
        "cwd": "{workspaceRoot}"
      }
    },
    "format:check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prettier --check \"apps/healthcare-events/**/*.{js,ts,json,md}\"",
        "cwd": "{workspaceRoot}"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["fhir-client"]
}
